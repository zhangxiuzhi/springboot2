.w160{
	width:160px;
}
.w80{
	width:80px;
}
/*---------------button---------------*/
.ui-btn{
	padding:6px 20px;
	font-size:12px;
	background:#e0e0e0;
	border:none;
	border-radius:2px;
	outline:none;
}
.btn-small{
	padding:6px 12px;
	font-size:12px;
}
.btn-white{
	color:#55585d;
	background:#fff;
	border:1px solid #efefef;
}
.btn-white:hover{
	background:#efefef;
}
.btn-white:focus{
	background:#e7e7e7;
}
.btn-blue{
	color:#fff;
	background:#4fbde9;
}
.btn-blue:hover{
	background:#34a4d1;
}
.btn-blue:focus{
	background:#2b94be;
}
.btn-red{
	color:#fff;
	background:#f17788;
}
.btn-red:hover{
	background:#e8566a;
}
.btn-red:focus{
	background:#dc4056;
}

.btn-linear{
	color:#4fbde9;
	border:1px solid #4fbde9;
	border-radius:2px;
	background:#fff;
}
.btn-linear:hover{
	color:#fff;
	background:#4fbde9;
}
.btn-linear:focus{
	color:#fff;
	background:#34a4d1;
}
/*--------input---------*/
.input-default{
    width:auto;
    border-radius:4px;
    border:1px solid #e5e5e5;
    color:#55585d;
    font-size:12px;
    height:34px;
    line-height:20px;
    padding:6px 10px;
}
/*--------dropdown---------*/
.ui-dropdown{
    width:auto;
    line-height:20px;
	height:34px;
    padding:6px 10px;
    border-radius:4px;
    background:#fff;
    border:1px solid #e5e5e5;
    color:#55585d;
    font-size:12px;
}
.ui-dropdown option:checked{
    background:#e6e6e6;
}
/*--------tab---------*/
.tab{
    background:#fff;
    padding:0 14px;
}
.tab .selected{
    color:#1f1f1f;
    font-size:14px;
    border-bottom:2px solid #4fbde9;
}
.tab-bottom{
    height:50px;
    background:#fff;
    border-bottom:1px solid #eeeeee;
}
.tab-bottom a{
    display:inline-block;
    color:#7a8995;
    font-size:12px;
    width:80px;
    height:48px;
    line-height:48px;
    text-align:center;
    vertical-align:middle;
    text-decoration:none;
    border-bottom:2px solid #fff;

}
.tab-bottom a:hover{
    color:#1f1f1f;
    font-size:14px;
    border-bottom:2px solid #4fbde9;
}
/*--------radio---------*/
.radio-default{
    color:#55585d;
    font-size:12px;
    width:13px;
    height:13px;
}
.radio-disabled{
    color:#55585d;
    font-size:12px;
    width:13px;
    height:13px;
    cursor:not-allowed;
    opacity:0.5;
}
.ui-radio{
    position:relative;
}
.ui-radio:checked:before{
    content:"";
    display:block;
    position:absolute;
    width:13px;
    height:13px;
    border-radius:50%;

}
.ui-radio:checked:after{
    content:"";
    display:block;
    position:absolute;
    top:4px;
    left:4px;
    width:5px;
    height:5px;
    border-radius:50%;
    background:#fff;
}
.radio-blue:checked:before{
    background:#2196f3;
}

.radio-green:checked:before{
    background:#63cb89;
}

/*--------checkbox---------*/
.ui-checkbox{
    position:relative;
}
.ui-checkbox:before{
    content:"";
    display:block;
    position:absolute;
    top:0;
    left:0;
    width:12px;
    height:12px;
    background:#e0e0e0;
    border:1px solid #b5b5b5;
    border-radius:2px;
}
.ui-checkbox:checked:after{
    content:"\f00c";
    font-family:FontAwesome;
    color:#fff;
    display:block;
    position:absolute;
    top:0;
    left:0;
    width:12px;
    height:12px;
    border-radius:2px;
	font-size:6px;
}
.checkbox-blue:checked:after{
    background:#2196f3;
    border:1px solid #2196f3;
}

.checkbox-green:checked:after{
    background:#63cb89;
    border:1px solid #63cb89;
}
.checkbox-default{
    position:relative;
}
.checkbox-default:before{
    content:"";
    display:block;
    position:absolute;
    top:0;
    left:0;
    width:12px;
    height:12px;
    background:#e0e0e0;
    border:1px solid #b5b5b5;
    border-radius:2px;
}
.checkbox-default:checked:after{
    content:"\f00c";
    font-family:FontAwesome;
    color:#646464;
    display:block;
    position:absolute;
    top:0;
    left:0;
    width:12px;
    height:12px;
    background:#e0e0e0;
    border:1px solid #b5b5b5;
    border-radius:2px;
}

/*---------------modal---------------*/
.modal .modal-head{
    padding:20px 40px;
    border-top-left-radius:4px;
    border-top-right-radius:4px;
    background:#eee;
    border-bottom:1px solid #e5e5e5;
    font-size:18px;
    color:#55585d;
}
.modal .modal-body{
    padding:20px 40px;
    background:#eee;
    font-size:12px;
    color:#55585d;
}
.modal .modal-foot{
    padding:20px 40px;
    border-bottom-left-radius:4px;
    border-bottom-right-radius:4px;
    background:#eee;
    text-align: center;
}

/*---------------paging---------------*/

.paging .paging-link{
	height:32px;
	color:#505b77;
	display:inline-block;
	font-size:12px;
	background:#f9fafc;
	border:1px solid #eee;
	border-radius:2px;
	margin-right:44px;
}
.paging .paging-link .num{
	display:inline-block;
	width:32px;
	height:32px;
	line-height:32px;
	vertical-align:middle;
	text-align:center;
	border-right:1px solid #eee;
	text-decoration:none;
	color:#505b77;
	margin:0 -2px;
}
.paging .paging-link .num:last-child{
	border:none;
}
.paging .paging-link .selected{
	background:#53a6fa;
	color:#fff;
}
.paging .paging-link a:hover{
	background:#53a6fa;
	color:#fff;
	border-right:1px solid #53a6fa;
}

.paging-dropdown{
	width:auto;
	height:32px;
	color:#1f1f1f;
	border:1px solid #ddd;
	border-radius:2px;
}
.paging-input{
	width:30px;
	height:32px;
	padding:0;
}
.paging .paging-pagination{
	margin-left:14px;
}
.paging .paging-redirect{
	margin-right:14px;
}
/*-------------------------table----------------------*/
.table{
	border-collapse:collapse;
