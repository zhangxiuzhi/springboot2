	border-right:1px solid #ddd;
	font-size:12px;
	color:#55585d;
	padding:25px 12px;
	text-align:left;
}
.table tbody td{
	font-size:12px;
	color:#666;
	border-bottom:1px solid #ddd;
	border-right:1px solid #ddd;
	padding:14px 12px;
}
.table tbody td{
	font-size:12px;
	color:#666;
	border-bottom:1px solid #ddd;
	border-right:1px solid #ddd;
	padding:14px 12px;
}
